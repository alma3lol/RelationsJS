{
  "name": "relationsjs",
  "version": "0.1.0",
  "private": true,
  "author": "Omar Almalol",
  "description": "Relations management & representation in JavaScript",
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "@mui/styles": "^5.2.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "assert": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "csv-parse": "^5.0.4",
    "electron-compile": "^6.4.4",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "graphology": "^0.23.2",
    "graphology-layout": "^0.5.0",
    "graphology-layout-forceatlas2": "^0.8.1",
    "graphology-types": "^0.23.0",
    "https-browserify": "^1.0.0",
    "i18next": "^21.6.4",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "lodash": "^4.17.21",
    "neo4j-driver": "^4.4.1",
    "notistack": "^2.0.3",
    "os": "^0.1.2",
    "os-browserify": "^0.3.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.11",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-sigma-v2": "^1.3.0",
    "react-use": "^17.3.2",
    "sigma": "^2.1.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "dev": "concurrently -k \"yarn start\" \"yarn electron\"",
    "electron": "electron-forge start -- --no-sandbox --remote-debugging-port=9222",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.3",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "devtron": "^1.4.0",
    "electron": "^16.0.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "electronmon": "^2.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "neo-forgery": "^2.0.0",
    "wait-for-localhost-cli": "^3.0.0",
    "wait-on": "^6.0.0"
  },
  "packageManager": "yarn@3.1.1",
  "build": {
    "appId": "com.alma3lol.relationsjs",
    "productName": "RelationsJS",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "Authors": "AlMA PRO",
            "exe": "captive-portal-js-ui-${version}.exe"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "AlMA PRO",
              "homepage": "https://github.com/almapro/CaptivePortalJS-ui"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "maintainer": "AlMA PRO",
              "homepage": "https://github.com/almapro/CaptivePortalJS-ui"
            }
          }
        }
      ],
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": true
      },
      "github_repository": {
        "owner": "almapro",
        "name": "CaptivePortalJS-ui"
      }
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
